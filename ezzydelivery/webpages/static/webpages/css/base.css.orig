@font-face {
  font-family: "Poppins-Medium";
  src: local("Poppins-Medium"),
    url("../fonts/Poppins-Medium.ttf") format("truetype");
}
@font-face {
  font-family: "Poppins-Light";
  src: local("Poppins-Light"),
    url("../fonts/Poppins-Light.ttf") format("truetype");
}

body {
  font-family: "Poppins-Light";
}

.btn-primary-fill {
  background-color: #ecc903;
  border: 0.06em solid #ffed4f;
  border-radius: 0.19em;
  color: #231e39;
  font-family: Poppins-Medium;
  font-weight: 500;
  padding: 0.6em 0.6em;
  width: 12em;
  box-sizing: none;
}
.btn-primary-fill:hover {
  background: #f7e64d;
  color: #231e39;
  border: 0.06em solid #ffc400;
  border-radius: 0.29em;
  transition: background 0.3s;
}

.btn-primary-fill:active {
  background: #fdfdf9;
  color: #231e39;
  border: 0.06em solid #ffc400;
  border-radius: 0.29em;
  transition: background 0.3s;
}

.btn-primary-blank {
  color: #5a5a5c;
  border: 1px solid #d7d8cf;
  border-radius: 0.19em;
  font-family: Poppins-Medium;
  font-weight: 500;
  width: 9em;
  padding: 0.6em 0.6em;
  background-color: #231e39;
  color: #eeeedc;
}
.btn-primary-blank:hover {
  background: #312b49;
  transition: background 0.3s;
}

.joinbtn {
  background-color: #83bfce !important;

  border-radius: 0.19em;
  color: #000000;
  font-family: Poppins-Medium;
  font-weight: 500;
  padding: 0.6em 0.6em;
  width: 15em;
  text-decoration: none;
}

.joinbtn.active {
  background-color: #013f4e !important;

  border-radius: 0.19em;
  color: #eeeedc;
  font-family: Poppins-Medium;
  font-weight: 500;
  padding: 0.6em 0.6em;
  width: 15em;
}

.follow {
  margin: 1.5em auto 0 auto;
  background-color: #ffd900;
  width: 150px;
  color: white;
  font-family: "Lato";
  text-align: center;
  padding: 0.5em;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
  -webkit-transition-property: all;
  -moz-transition-property: all;
  -o-transition-property: all;
  transition-property: all;
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  -o-transition-duration: 0.3s;
  transition-duration: 0.3s;
  -webkit-transition-timing-function: ease;
  -moz-transition-timing-function: ease;
  -o-transition-timing-function: ease;
  transition-timing-function: ease;
}
.follow:hover {
  background-color: #167abd;
  cursor: pointer;
}

@media screen and (min-width: 768px) {
  .footerlogo {
    width: 150px !important;
    height: 135px !important;
  }
}
